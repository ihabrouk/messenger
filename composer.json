{
    "name": "ihabrouk/messenger",
    "description": "A comprehensive Laravel package for multi-provider messaging (SMS, WhatsApp) with FilamentPHP integration",
    "type": "library",
    "keywords": [
        "laravel",
        "messaging",
        "sms",
        "whatsapp",
        "filament",
        "notifications",
        "bulk-messaging",
        "templates"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ismail El Habrouk",
            "email": "ismailhabrouk@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "filament/filament": "^3.3",
        "guzzlehttp/guzzle": "^7.8",
        "twilio/sdk": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Ihabrouk\\Messenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ihabrouk\\Messenger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ihabrouk\\Messenger\\MessengerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
